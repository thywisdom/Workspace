package gupta;

public class Largestof3num {

	public static void main(String[] args) {
		Scanner scan=new scan(System.in);
	   int a=45;b=67;c=88;
	   if(a>b && a>c) {
		   System.out.println("a is greater");
	   }
	   else if(b>a && b>c) {
		   System.out.println("b is greater");
	   }
	   else if(c>b && c>a) {
		   System.out.println("c is greater");
	   }
	   
	   
	   

	}

}
