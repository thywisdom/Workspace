package gupta;

import java.util.Scanner;

public class Inna {

	public static void main(String[] args) {
		int num=34;
		
		Scanner scan=new Scanner(System.in);
		if (num%2==0) {
		 System.out.println("The entered number is even");
		}
		else {
			System.out.println("The entered number is odd");
			
		}
		

	}

}
